{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "nixpacks"
  },
  "deploy": {
    "startCommand": "java -Xmx400m -XX:+UseG1GC -XX:+UseStringDeduplication -Dspring.profiles.active=railway -jar target/data-insights-portal-0.0.1-SNAPSHOT.jar",
    "healthcheckPath": "/actuator/health",
    "healthcheckTimeout": 120,
    "restartPolicyType": "on_failure"
  }
}